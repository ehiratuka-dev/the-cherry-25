{
    "name": "The Cherry 25",
    "image": "mcr.microsoft.com/devcontainers/typescript-node:latest",
    "appPort": [3000],
    "customizations": {
        "vscode": {
            "settings": {
                "terminal.integrated.shell.linux": "/bin/bash"
            },
            "extensions": [
                "dbaeumer.vscode-eslint"
            ]
        }
    },
    "postCreateCommand": "npm install",
    "mounts": [
        "source=the-cherry-25-volume,target=/workspaces/the-cherry-25/public/data,type=volume",
        "source=the-cherry-25-webapp,target=/workspaces/the-cherry-25/dist,type=volume"
    ]
}
